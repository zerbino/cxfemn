<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from ServiceImplService WSDL service -->

  <service name="ServiceImpl" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://model"/>
      <parameter name="wsdlServiceElement" value="ServiceImplService"/>
      <parameter name="schemaQualified" value="http://model"/>
      <parameter name="wsdlServicePort" value="ServiceImpl"/>
      <parameter name="className" value="model.ServiceImplSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="ServiceImpl"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="op" qname="operNS:op" xmlns:operNS="http://model" returnQName="retNS:opReturn" xmlns:retNS="http://model" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:p" xmlns:pns="http://model" type="tns:Personne" xmlns:tns="http://model"/>
      </operation>
      <parameter name="allowedMethods" value="op"/>

      <typeMapping
        xmlns:ns="http://model"
        qname="ns:Personne"
        type="java:model.Personne"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
